<?xml version="1.0" encoding="UTF-8"?>




<ui-table xmlns="http://www.efaps.org/xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>efe5b94f-4029-443e-93e2-f58560a40308</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_Access_AccessSet2UserAbstractTable</name>
        

        <field name="name">
            <property name="SelectAlternateOID">linkto[UserAbstractLink].oid</property>
            <property name="Select">linkto[UserAbstractLink].attribute[Name]</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_User_Abstract/Name.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="status">
            <property name="Select">linkto[UserAbstractLink].attribute[Status]</property>
            <property name="Label">Admin_User_Abstract/Status.Label</property>
            <property name="FilterType">PickList</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="SelectAlternateOID">linkto[UserAbstractLink].oid</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>
</ui-table>
