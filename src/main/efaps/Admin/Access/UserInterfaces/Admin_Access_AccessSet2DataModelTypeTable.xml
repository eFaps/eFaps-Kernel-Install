<?xml version="1.0" encoding="UTF-8"?>




<ui-table xmlns="http://www.efaps.org/xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>fe2dca11-8b21-4a80-b459-236db014bd1c</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_Access_AccessSet2DataModelTypeTable</name>
        

        <field name="name">
            <property name="SelectAlternateOID">linkto[DataModelTypeLink].oid</property>
            <property name="Select">linkto[DataModelTypeLink].attribute[Name]</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_DataModel_Type/Name.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="uuid">
            <property name="Select">linkto[DataModelTypeLink].attribute[UUID]</property>
            <property name="Label">Admin_DataModel_Type/UUID.Label</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="parentType">
            <property name="SelectAlternateOID">linkto[DataModelTypeLink].linkto[ParentType].oid</property>
            <property name="Select">linkto[DataModelTypeLink].linkto[ParentType].attribute[Name]</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_DataModel_Type/ParentType.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="SelectAlternateOID">linkto[DataModelTypeLink].oid</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>

</ui-table>
