<?xml version="1.0" encoding="UTF-8"?>




<ui-table xmlns="http://www.efaps.org/xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>1798a388-768b-4ceb-95ce-99498da44fb3</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_Access_AccessSet2TypeTable</name>
        

        <field name="name">
            <property name="SelectAlternateOID">linkto[AccessTypeLink].oid</property>
            <property name="Select">linkto[AccessTypeLink].attribute[Name]</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_Access_AccessType/Name.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="uuid">
            <property name="Select">linkto[AccessTypeLink].attribute[UUID]</property>
            <property name="Label">Admin_Access_AccessType/UUID.Label</property>
            <property name="ModeSearch">READONLY</property>
        </field>
        <field name="revision">
            <property name="Select">linkto[AccessTypeLink].attribute[Revision]</property>
            <property name="Label">Admin_Access_AccessType/Revision.Label</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="SelectAlternateOID">linkto[AccessTypeLink].oid</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>

</ui-table>
