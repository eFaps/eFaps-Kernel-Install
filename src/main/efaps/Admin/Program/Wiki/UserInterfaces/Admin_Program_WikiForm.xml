<?xml version="1.0" encoding="UTF-8"?>




<ui-form xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>217759ce-50ed-46b0-8a0c-05db0151ea92</uuid>
    <file-application>eFaps-Kernel</file-application>
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_Program_WikiForm</name>
        

        <field name="type">
            <property name="Attribute">Type</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="name">
            <property name="Columns">40</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="Attribute">Name</property>
            <property name="Required">true</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
        <field name="revision">
            <property name="Columns">20</property>
            <property name="ModeCreate">EDITABLE</property>
            <property name="ModeEdit">EDITABLE</property>
            <property name="Attribute">Revision</property>
            <property name="Required">true</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
        <field name="createGroup" character="Group">
            <property name="GroupCount">2</property>
        </field>
        <field name="creator">
            <property name="SelectAlternateOID">linkto[Creator].oid</property>
            <property name="Attribute">Creator</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="created">
            <property name="Attribute">Created</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
        <field name="modifyGroup" character="Group">
            <property name="GroupCount">2</property>
        </field>
        <field name="modifier">
            <property name="SelectAlternateOID">linkto[Modifier].oid</property>
            <property name="Attribute">Modifier</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="modified">
            <property name="Attribute">Modified</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
        <field name="wiki">
            <property name="ModeEdit">NONE</property>
            <property name="UIType">SNIPPLET</property>
            <property name="UIProvider">org.efaps.admin.datamodel.ui.StringUI</property>
            <trigger name="Admin_Program_WikiForm.css.UI_FIELD_VALUE"
                     event="UI_FIELD_VALUE"
                     program="org.efaps.esjp.admin.program.FormatOutput"
                     method="wiki" />
        </field>
        <field name="compiledheader" character="Heading">
            <property name="ModeEdit">NONE</property>
            <property name="Level">1</property>
            <property name="Label">Admin_Program_WikiForm.compiledheader.Heading</property>
        </field>
        <field name="compiledtable" character="Target">
            <property name="ModeEdit">NONE</property>
            <table>Admin_Program_JavaClassTable</table>
            <evaluate program="org.efaps.esjp.common.uitable.MultiPrint">
                <property name="Type">Admin_Program_WikiCompiled</property>
                <property name="LinkFrom">ProgramLink</property>
            </evaluate>
        </field>
        <field name="linkheader" character="Heading">
            <property name="ModeEdit">NONE</property>
            <property name="Level">1</property>
            <property name="Label">Admin_Program_WikiForm.linkheader.Heading</property>
        </field>
        <field name="extendtable" character="Target">
            <property name="ModeEdit">NONE</property>
            <table>Admin_Program_AbstractTable</table>
            <evaluate program="org.efaps.esjp.common.uitable.MultiPrint">
                <property name="Type">Admin_Program_Wiki2Wiki</property>
                <property name="LinkFrom">From</property>
                <property name="InstanceSelect">linkto[To].instance</property>
            </evaluate>
        </field>
        <field name="htmlheader" character="Heading">
            <property name="ModeEdit">NONE</property>
            <property name="Level">1</property>
            <property name="Label">Admin_Program_WikiForm.htmlheader.Heading</property>
        </field>
        <field name="wikiHtml">
            <property name="UIProvider">org.efaps.admin.datamodel.ui.StringUI</property>
            <property name="ModeEdit">NONE</property>
            <property name="Label">Admin_Program_WikiForm.wikiHtml.Label</property>
            <trigger name="Admin_Program_WikiForm.css.UI_FIELD_VALUE"
                     event="UI_FIELD_VALUE"
                     program="org.efaps.esjp.admin.program.FormatOutput"
                     method="wikiHtml" />
        </field>
    </definition>
</ui-form>
