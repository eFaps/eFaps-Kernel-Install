<?xml version="1.0" encoding="UTF-8"?>




<ui-table xmlns="http://www.efaps.org/xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>3ec50b4d-3c21-4be8-9518-c1d516888f3f</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_DataModel_SQLTableTable</name>
        

        <field name="name">
            <property name="Attribute">Name</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_DataModel_SQLTable/Name.Label</property>
            <property name="ShowTypeIcon">true</property>
            <property name="Width">3</property>
        </field>
        <field name="uuid">
            <property name="Attribute">UUID</property>
            <property name="Label">Admin_DataModel_SQLTable/UUID.Label</property>
            <property name="Width">3</property>
        </field>
        <field name="SQLTable">
            <property name="Attribute">SQLTable</property>
            <property name="Label">Admin_DataModel_SQLTable/SQLTable.Label</property>
            <property name="Width">3</property>
        </field>
        <field name="SQLColumnID">
            <property name="Attribute">SQLColumnID</property>
            <property name="Label">Admin_DataModel_SQLTable/SQLColumnID.Label</property>
            <property name="Width">1</property>
        </field>
        <field name="SQLColumnType">
            <property name="Attribute">SQLColumnType</property>
            <property name="Label">Admin_DataModel_SQLTable/SQLColumnType.Label</property>
            <property name="Width">1</property>
        </field>
        <field name="DMTableMain">
            <property name="SelectAlternateOID">linkto[DMTableMain].oid</property>
            <property name="Select">linkto[DMTableMain].attribute[Name]</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Label">Admin_DataModel_SQLTable/DMTableMain.Label</property>
            <property name="Width">3</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
            <property name="Width">15px</property>
        </field>
    </definition>

</ui-table>
