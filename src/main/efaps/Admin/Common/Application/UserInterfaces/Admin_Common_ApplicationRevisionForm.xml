<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<ui-form xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">
    <uuid>30484e1c-c0ea-409d-b684-7f33ce497cda</uuid>
    <file-application>eFaps-Kernel</file-application>
        <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Admin_Common_ApplicationRevisionForm</name>
        <field name="application">
            <property name="Select">linkto[ApplicationLink].attribute[Name]</property>
        </field>
        <field name="revision">
            <property name="Attribute">Revision</property>
        </field>
        <field name="date">
            <property name="Attribute">Date</property>
        </field>
        <field character="Group" name="createGroup">
            <property name="GroupCount">2</property>
        </field>
        <field name="creator">
            <property name="SelectAlternateOID">linkto[Creator].oid</property>
            <property name="Attribute">Creator</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="created">
            <property name="Attribute">Created</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
        <field character="Group" name="modifyGroup">
            <property name="GroupCount">2</property>
        </field>
        <field name="modifier">
            <property name="SelectAlternateOID">linkto[Modifier].oid</property>
            <property name="Attribute">Modifier</property>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ModeSearch">EDITABLE</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="modified">
            <property name="Attribute">Modified</property>
            <property name="ModeSearch">EDITABLE</property>
        </field>
    </definition>
</ui-form>
