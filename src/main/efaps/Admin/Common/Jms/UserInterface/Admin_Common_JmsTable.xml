            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="ShowTypeIcon">true</property>
        </field>
        <field name="esjp">
            <property name="SelectAlternateOID">linkto[ESJPLink].oid</property>
            <property name="Select">linkto[ESJPLink].attribute[Name]</property>
            <property name="Label">Admin_Common_JmsTable/ESJPLink.Label</property>
        </field>
        <field name="connectionFactoryJNDI">
            <property name="Attribute">ConnectionFactoryJNDI</property>
            <property name="Label">Admin_Common_JmsTable/ConnectionFactoryJNDI.Label</property>
        </field>
        <field name="destinationJNDI">
            <property name="Attribute">DestinationJNDI</property>
            <property name="Label">Admin_Common_JmsTable/DestinationJNDI.Label</property>
        </field>
        <field name="buttonOpenInNewWindow">
            <icon>eFapsActionNewWindow</icon>
            <property name="HRef">${COMMONURL}/MenuTree.jsp</property>
            <property name="Target">popup</property>
            <property name="SortAble">false</property>
