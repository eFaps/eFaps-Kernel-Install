<import xmlns="http://www.efaps.org/xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <!-- Karsten Kunkel -->
    <object type="Admin_User_Person">
        <attribute name="Name">QuartzTrigger</attribute>
        <attribute name="FirstName">The</attribute>
        <attribute name="LastName">QuartzTrigger</attribute>
        <attribute name="TimeZone">UTC</attribute>
        <attribute name="Status">true</attribute>
        <attribute name="Locale">en</attribute>
        <linkattribute name="Language" type="Admin_Language">
            <queryattribute name="Language">en</queryattribute>
        </linkattribute>
    </object>

    <object type="Admin_User_Person2Role">
        <linkattribute name="UserFromLink" type="Admin_User_Person">
            <queryattribute name="Name">QuartzTrigger</queryattribute>
        </linkattribute>
        <linkattribute name="UserToLink" type="Admin_User_RoleGlobal">
            <queryattribute name="Name">Administration</queryattribute>
        </linkattribute>
        <linkattribute name="UserJAASSystem" type="Admin_User_JAASSystem">
            <queryattribute name="Name">eFaps</queryattribute>
        </linkattribute>
    </object>
</import>
