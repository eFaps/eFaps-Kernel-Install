<?xml version="1.0" encoding="UTF-8"?>




<ui-command xmlns="http://www.efaps.org/xsd"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>5a6f2d4a-df81-4211-b7ed-18ae83608c81</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Message_MessageAlert</name>

        

        <target>
            <menu>Message_MessageAlert_Menu</menu>
            <table>Message_MessageAlertTable</table>
            <evaluate program="org.efaps.esjp.message.Message" method="showAlertMessage">
                <property name="Type">Message_Message2User</property>
                <property name="ExpandChildTypes">true</property>
            </evaluate>
        </target>
        

        <property name="Target">modal</property>
        <property name="TargetTableSortKey">name</property>
        <property name="TargetShowCheckBoxes">true</property>
    </definition>

</ui-command>
