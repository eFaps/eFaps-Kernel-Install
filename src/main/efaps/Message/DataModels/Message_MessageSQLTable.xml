<?xml version="1.0" encoding="UTF-8"?>




<datamodel-sqltable xmlns="http://www.efaps.org/xsd"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://www.efaps.org/xsd http://www.efaps.org/xsd/eFaps_1.0.xsd">

    <uuid>58fb41ba-3820-4753-a48a-ecf024fea79d</uuid>
    <file-application>eFaps-Kernel</file-application>
    
    

    <definition>
        <version-expression>(version==latest)</version-expression>
        <name>Message_MessageSQLTable</name>
        <typeid-column>TYPEID</typeid-column>
        <database>
            <table-name>T_MSG</table-name>
            

            <column name="TYPEID"    type="INTEGER"                      not-null="true" />
            <column name="DESCR"     type="STRING_SHORT" length="1024"   not-null="true" />
            <column name="VALUE"     type="CLOB"  	                     not-null="true" />
            <column name="PRIORITY"  type="INTEGER" />
            <column name="KEY"       type="STRING_SHORT" length="1024"                />
            <column name="CREATOR"   type="INTEGER"                      not-null="true" />
            <column name="CREATED"   type="DATETIME"                     not-null="true" />
            <column name="MODIFIER"  type="INTEGER"                      not-null="true" />
            <column name="MODIFIED"  type="DATETIME"                     not-null="true" />
            

            <foreign name="T_MSG_FK_CRTR" key="CREATOR" reference="T_USERPERSON(ID)" />
            <foreign name="T_MSG_FK_MDFR" key="MODIFIER" reference="T_USERPERSON(ID)" />
        </database>
    </definition>
</datamodel-sqltable>
